Bundle-Name: SAI :: RequireJS :: Web Resource

Bundle-Description: \
	Provides web resources for RequireJS applications

requirejs.version = 2.3.3

-buildpath: \
	osgi.enroute.web.simple.provider

-includeresource: \
	@${repo;org.webjars.npm:requirejs;${requirejs.version}}!/META-INF/resources/*

Export-Package: \
	uk.co.saiman.requirejs
