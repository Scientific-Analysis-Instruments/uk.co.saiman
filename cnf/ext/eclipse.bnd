## Repository Plugins:
repository.eclipse.releases: http://download.eclipse.org/releases/mars/
repository.eclipse.wtp: http://download.eclipse.org/webtools/repository/mars/
repository.eclipse.updates:  http://download.eclipse.org/eclipse/updates/4.5/
repository.eclipse.fx.tooling: http://download.eclipse.org/efxclipse/updates-released/2.4.0/site/
repository.eclipse.fx.runtime: http://download.eclipse.org/efxclipse/runtime-released/2.4.0/site/

plugin.eclipse.releases:\
        aQute.bnd.repository.p2.provider.P2Repository;\
			name				=		"Eclipse Simultaneous Release Mars";\
			url					=		${repository.eclipse.releases}

plugin.eclipse.updates:\
        aQute.bnd.repository.p2.provider.P2Repository;\
        	name				=		"Eclipse Platform 4.5.x";\
			url					=		${repository.eclipse.updates}

plugin.eclipse.fx.tooling:\
        aQute.bnd.repository.p2.provider.P2Repository;\
        	name				=		"E(fx)clipse 2.4.0 Tooling";\
			url					=		${repository.eclipse.fx.tooling}

plugin.eclipse.fx.runtime:\
        aQute.bnd.repository.p2.provider.P2Repository;\
        	name				=		"E(fx)clipse 2.4.0 Runtime";\
			url					=		${repository.eclipse.fx.runtime}

plugin.eclipse.wtp:\
        aQute.bnd.repository.p2.provider.P2Repository;\
			name				=		"Eclipse WTP Mars";\
			url					=       ${repository.eclipse.wtp}

