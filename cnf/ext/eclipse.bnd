## Run properties groups:
runprops.eclipse:\
	osgi.console=,\
	eclipse.consoleLog=true,\
	equinox.use.ds=true,\
	osgi.instance.area.default=./workspace,\
	osgi.console.enable.builtin=true

runprops.eclipse.fx:\
	osgi.framework.extensions=\
		org.eclipse.fx.osgi

runprops.eclipse.clean:\
	osgi.clean=true,\
	clearPersistedState=true

