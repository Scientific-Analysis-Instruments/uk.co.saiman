
runplatform: linux64








## Run path groups:
-runpath.eclipse.fx:\
	org.eclipse.fx.osgi

-augment.org.eclipse.fx.ui.services:\
	org.eclipse.fx.ui.services;\
		capability:="org.eclipse.fx.ui.services;version=1.2.0"

## Run requirement groups:
-runrequiresNO.eclipse.fx:\
	bnd.identity; id=org.eclipse.fx.ui.databinding,\
	bnd.identity; id=org.eclipse.fx.ui.di,\
	bnd.identity; id=org.eclipse.fx.ui.di.interopt,\
	bnd.identity; id=org.eclipse.fx.ui.keybindings.e4,\
	bnd.identity; id=org.eclipse.fx.ui.keybindings.generic,\
	bnd.identity; id=org.eclipse.fx.ui.workbench.fx,\
	bnd.identity; id=org.eclipse.fx.ui.workbench.renderers.fx,\
	bnd.identity; id=org.eclipse.fx.core,\
	bnd.identity; id=org.eclipse.fx.core.di,\
	bnd.identity; id=org.eclipse.fx.core.di.context

-runrequiresNO.eclipse.fx.p2:\
	bnd.identity; id=org.eclipse.fx.core.p2

## Run properties groups:
-runproperties.eclipse.fx:\
	osgi.framework.extensions=\
		org.eclipse.fx.osgi

## Run system packages groups:
-runsystempackages.eclipse.fx:\
	com.sun.glass.ui,\
	javafx.animation,\
	javafx.application,\
	javafx.beans,\
	javafx.beans.binding,\
	javafx.beans.property,\
	javafx.beans.property.adapter,\
	javafx.beans.value,\
	javafx.collections,\
	javafx.collections.transformation,\
	javafx.concurrent,\
	javafx.css,\
	javafx.embed.swing,\
	javafx.embed.swt,\
	javafx.event,\
	javafx.fxml,\
	javafx.geometry,\
	javafx.scene,\
	javafx.scene.canvas,\
	javafx.scene.chart,\
	javafx.scene.control,\
	javafx.scene.control.cell,\
	javafx.scene.effect,\
	javafx.scene.image,\
	javafx.scene.input,\
	javafx.scene.layout,\
	javafx.scene.media,\
	javafx.scene.paint,\
	javafx.scene.shape,\
	javafx.scene.text,\
	javafx.scene.transform,\
	javafx.scene.web,\
	javafx.stage,\
	javafx.util,\
	javafx.util.converter,\
	netscape.javascript





















## Run path groups:
-runpath.eclipse:\
	${def;runpath.eclipse.${runplatform}},\
\
	org.eclipse.osgi.compatibility.state

runpath.eclipse.win64:\
	org.eclipse.equinox.launcher.win32.win32.x86_64

runpath.eclipse.linux64:\
	org.eclipse.equinox.launcher.gtk.linux.x86_64

-augment.org.eclipse.core.runtime:\
	org.eclipse.core.runtime;\
		capability:="osgi.service;objectClass:List<String>=org.eclipse.core.runtime.IAdapterManager;effective:=active"

## Run requirement groups:
-runrequires.eclipse:\
	bnd.identity; id=bndtools.runtime.eclipse.applaunch; version=latest,\
\
	${def;runrequires.eclipse.${runplatform}},\
\
	bnd.identity; id=org.eclipse.core.databinding.beans; version=latest,\
	bnd.identity; id=org.eclipse.emf.databinding; version=latest,\
	bnd.identity; id=org.eclipse.equinox.app; version=latest,\
	bnd.identity; id=org.eclipse.equinox.concurrent; version=latest,\
	bnd.identity; id=org.eclipse.equinox.console; version=latest,\
	bnd.identity; id=org.eclipse.equinox.event; version=latest,\
	bnd.identity; id=org.eclipse.equinox.launcher; version=latest,\
	bnd.identity; id=org.eclipse.e4.ui.bindings; version=latest

runrequires.eclipse.win64:\
	bnd.identity; id=org.eclipse.core.filesystem.win32.x86_64; version=latest,\
	bnd.identity; id=org.eclipse.core.resources.win32.x86_64; version=latest,\
	bnd.identity; id=org.eclipse.equinox.security.win32.x86_64; version=latest,\
	bnd.identity; id=org.eclipse.equinox.launcher.win32.win32.x86_64; version=latest

runrequires.eclipse.linux64:\
	bnd.identity; id=org.eclipse.core.filesystem.linux.x86_64; version=latest,\
	bnd.identity; id=org.eclipse.core.net.linux.x86_64; version=latest,\
	bnd.identity; id=org.eclipse.equinox.launcher.gtk.linux.x86_64; version=latest

## Run properties groups:
-runproperties.eclipse:\
	osgi.console=,\
	eclipse.consoleLog=true,\
	equinox.use.ds=true,\
	osgi.instance.area.default=./workspace,\
	osgi.console.enable.builtin=true





















-runvm.msapex:\
	-Xmx1024m

-runproperties:\
	osgi.ws=gtk,\
	osgi.os=linux,\
	osgi.arch=x86_64,\
	eclipse.product=uk.co.saiman.msapex.product,\
	uk.co.saiman.configadmin.persistence=~/.saiman/config
