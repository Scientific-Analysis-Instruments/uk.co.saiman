<?xml version="1.0" ?>
<schema
	xmlns="http://modabi.strangeskies.co.uk/2014-01-01/"
	xmlns:sai="http://saiman.co.uk/2015-10-08/"
	name="sai:UtilitiesSchema">

	<imports>
		uk.co.strangeskies.reflection.TypeToken.Infer,
		uk.co.saiman.utilities.Configurable
	</imports>

	<dependencies>
		BaseSchema
	</dependencies>

	<types />

	<models>
		<model
			name="sai:configurable"
			abstract="true"
			dataType="@Infer Configurable&lt;?&gt;"
			bindingType="@Infer Configurable&lt;?&gt;"
			unbindingType="@Infer Configurable&lt;?&gt;">

			<complex
				name="sai:configuration"
				abstract="true"
				extensible="true"
				outMethod="configuration"
				dataType="@Infer ?"
				unbindingStrategy="SIMPLE"
				inMethod="configure">
			</complex>
		</model>
	</models>
</schema>
