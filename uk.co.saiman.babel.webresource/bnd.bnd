Bundle-Name: SAI :: Babel :: Web Resource

Bundle-Description: \
	Provides web resources for Babel applications

core.version = 6.24.1
standalone.version = 6.24.0

-buildpath: \
	osgi.enroute.web.simple.provider

-includeresource: \
	@${repo;org.webjars.npm:babel-core;${core.version}}!/META-INF/resources/*,\
	@${repo;org.webjars.npm:babel-standalone;${standalone.version}}!/META-INF/resources/*

Export-Package: \
	uk.co.saiman.babel.core,\
	uk.co.saiman.babel.standalone
