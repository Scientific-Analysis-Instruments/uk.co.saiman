Bundle-Name: SAI :: Web Console
Bundle-SymbolicName: uk.co.saiman.webconsole

Bundle-Description: \
	Provides a web console for SAI system administration.

root: /static/sai/webconsole

EnRoute-Application: \
	system/console

-includeresource: \
	${root}/sai-webconsole.js=static/sai-webconsole.js,\
	/META-INF/webconsole.components.properties=properties/webconsole.components.properties

Import-Package: \
	org.apache.felix.webconsole

Export-Package: \
	uk.co.saiman.webconsole
